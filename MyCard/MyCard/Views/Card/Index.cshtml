<!-- Container for the image gallery -->
<div class="container-fluid-my">

    <!-- Full-width images with number text -->
    <div class="mySlides fade-my">
        <div class="numbertext">1 / 5</div>
        <img src="~/img/slider1.jpg" style="width:100%; height:350px;">
    </div>

    <div class="mySlides fade-my">
        <div class="numbertext">2 / 5</div>
        <img src="~/img/slider2.jpg" style="width:100%; height:350px;">
    </div>

    <div class="mySlides fade-my">
        <div class="numbertext">3 / 5</div>
        <img src="~/img/slider3.jpg" style="width:100%; height:350px;">
    </div>

    <div class="mySlides fade-my">
        <div class="numbertext">4 / 5</div>
        <img src="~/img/slider4.jpg" style="width:100%; height:350px;">
    </div>

    <div class="mySlides fade-my">
        <div class="numbertext">5 / 5</div>
        <img src="~/img/slider5.jpeg" style="width:100%; height:350px;">
    </div>

    <!-- Next and previous buttons -->
    <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
    <a class="next" onclick="plusSlides(1)">&#10095;</a>

    <!-- Image text -->
    <div class="caption-container">
        <p id="caption"></p>
    </div>

    <!-- Thumbnail images -->
    <div class="row">
        <div class="column">
            <img class="demo cursor" src="~/img/slider1Mini.jpg" style="width:100%;height:80px" onclick="currentSlide(1)" alt="Lorem ipsum">
        </div>
        <div class="column">
            <img class="demo cursor" src="img/slider2Mini.jpg" style="width:100%;height:80px" onclick="currentSlide(2)" alt="Lorem ipsum">
        </div>
        <div class="column">
            <img class="demo cursor" src="img/slider3Mini.jpg" style="width:100%;height:80px" onclick="currentSlide(3)" alt="Lorem ipsum">
        </div>
        <div class="column">
            <img class="demo cursor" src="~/img/slider4Mini.jpg" style="width:100%;height:80px" onclick="currentSlide(4)" alt="Lorem ipsum">
        </div>
        <div class="column">
            <img class="demo cursor" src="~/img/slider5Mini.jpg" style="width:100%;height:80px" onclick="currentSlide(5)" alt="Lorem ipsum">
        </div>
    </div>
</div>
<br>

<div style="text-align:center">
    <span class="dot"></span>
    <span class="dot"></span>
    <span class="dot"></span>
</div>
<div class="container">
    <hr />
    <div class="row border-right border-left border-light" id="hobby">
        <div class="col-12  blackBG ">
            <p class="text-center h3 font-weight-bold paddingTopBot-8">Zainteresowania</p>
        </div>
    </div>
    <div class="radio-btns" role="radiogroup">

        <div class="row h-25 border-right border-left border-light ">
            <div class="col-md-6 col-sm-12 p-1 blackBG myTiles px-md-2 ">
                @*<div class="col-md-3 col-sm-6 col-xs-12 p-1 myTiles">*@
                <div class="radio-btns__btn" role="radio" aria-checked="false" tabindex="-1" aria-label="Select image one">

                    <div class="m-1">
                        <img class="img-fluid rounded mx-auto d-block " src="~/img/sw.png" />
                    </div>
                    @*</div>*@
                    @*<div class="col-md-3 col-sm-6 col-xs-12 p-2 myTiles">*@
                    <div class="m-1">
                        <p class="text-justify"><span class="font-weight-bold">Gwiezdne wojny </span>– amerykańska franczyza filmowa z gatunku space opera oparta na trylogii filmów stworzonej przez George’a Lucasa. Franczyza została zapoczątkowana w 1977 roku premierą filmu Star Wars.</p>
                    </div>
                </div>

                @*</div>*@
            </div>
            <div class="col-md-6 col-sm-12 p-1 blackBG myTiles px-md-2 ">
                @*<div class="col-md-3 col-sm-6 col-xs-12 p-1 myTiles">*@
                <div class="radio-btns__btn" role="radio" aria-checked="false" tabindex="-1" aria-label="Select image one">

                    <div class="m-1">
                        <img class="img-fluid rounded mx-auto d-block " src="~/img/it.jpg" />
                    </div>
                    @*</div>*@
                    @*<div class="col-md-3 col-sm-6 col-xs-12 p-2 myTiles">*@
                    <div class="m-1">
                        <p class="text-justify"><span class="font-weight-bold">Programowanie</span> – proces tworzenia programu komputerowego, który ma realizować określone zadania. Kod źródłowy jest napisany w języku programowania.Między programistami trwają debaty, czy programowanie komputerów jest sztuką, rzemiosłem czy procesem inżynieryjnym.</p>
                    </div>
                </div>

                @*</div>*@
            </div>
        </div>
        <div class="row h-25  border-right border-left border-light">
            <div class="col-md-6 col-sm-12 blackBG p-1 myTiles px-md-2  ">

                @*<div class="col-md-3 col-sm-6 col-xs-12 p-1 myTiles">*@
                <div class="radio-btns__btn" role="radio" aria-checked="false" tabindex="-1" aria-label="Select image one">
                    <div class="m-1">
                        <img class="img-fluid rounded mx-auto d-block " src="~/img/dice.jpg" />
                    </div>
                    @*</div>*@
                    @*<div class="col-md-3 col-sm-6 col-xs-12 p-2 myTiles">*@
                    <div class="m-1">
                        <p class="text-justify"><span class="font-weight-bold">Gra fabularna</span>- (inaczej RPG, z ang. role-playing game, nieraz zwana grą wyobraźni, potocznie erpegiem lub rolplejem) – gra towarzyska oparta na narracji, w której gracze (od jednego do kilku) wcielają się w role fikcyjnych postaci</p>
                    </div>
                </div>

                @*</div>*@
            </div>
            <div class="col-md-6 col-sm-12 blackBG p-1 myTiles px-md-2 ">
                @*<div class="col-md-3 col-sm-6 col-xs-12 p-1 myTiles">*@
                <div class="radio-btns__btn" role="radio" aria-checked="false" tabindex="-1" aria-label="Select image one">
                    <div class="m-1">
                        <img class="img-fluid rounded mx-auto d-block" src="~/img/sycho.jpg" />
                    </div>
                    @*</div>*@
                    @*<div class="col-md-3 col-sm-6 col-xs-12 p-2 myTiles">*@
                    <div class="m-1">
                        <p class="text-justify"><span class="font-weight-bold">Psychologia </span> - (od stgr. ψυχή psyche „dusza”; λόγος logos „słowo, myśl, rozumowanie”) – nauka badająca mechanizmy i prawa rządzące psychiką oraz zachowaniami człowieka. Psychologia bada również wpływ zjawisk psychicznych na interakcje międzyludzkie oraz interakcję z otoczeniem. </p>
                    </div>
                </div>

                @*</div>*@
            </div>
        </div>
    </div>
    <hr />
    <div class="row border-right border-left border-light" id="rss">
        @foreach (var news in Model)
        {
            <div class="col-md-6 col-sm-12 blackBG px-md-5">
                <h5 class="p-2">@news.Title</h5>
                <p>Publication date: @news.PubDate</p>
                <p>
                    Categories:<br />
                    @foreach (var cat in news.Categories)
                    {
                        @cat<br />
                    }
                </p>
                <p>Description: @news.Description</p>
                <p>Link: <a href="@news.Link" target="_blank">ReadMore</a></p>
            </div>
        }
    </div>
    <hr />
    <div class="row" id="mapSaber">
        <div class="col-12 ">
            <div id="map" class="border border-light"></div>
        </div>
    </div>
</div>

@section Scripts{
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCND8l4ZzlxpXIbEbAipD41tKcpBMpAGsA&callback=initMap" async></script>

}
